def powerOfTwoIt(n):
    i = 0
    power = 1
    while i < n:
        power = power * 2
        i = i + 1
    return power
kQ=powerOfTwoIt(4)
print(kQ)

# nhập powerOfTwoIt(4) , n=4 
# thực hiện lệnh i = 0 +1 , i=1 => power =1*2 =2
# i=1+1 , i=2 => power = 2 *2 = 4
#i=2+1 , i=3 => power =4*2 = 8
#i=3+1 ,i=4 => power = 8*2 = 16 
# in ra mang hinh ket qua 16
